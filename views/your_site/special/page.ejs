<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= pname %></title>
</head>
<body>
    <%- navbar %>
    <div class="hero">
        <h1><%= pname %></h1>
        <div class="cts">
            <% pages.forEach(page => { %>
                <span><a href="<%= page.url %>"><%= page.name %></a></span>
            <% }) %>
        </div>
    </div>
    <div class="container">
        <%- content %>
    </div>
</body>
</html>